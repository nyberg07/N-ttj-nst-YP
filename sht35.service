[Unit]
Description=SHT35 Temperature and Humidity Logger
After=network.target

[Service]
User=debian
WorkingDirectory=/home/debian
ExecStart=/home/debian/sht35_venv/bin/python /home/debian/sht35.py
Restart=always
RestartSec=5
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target

